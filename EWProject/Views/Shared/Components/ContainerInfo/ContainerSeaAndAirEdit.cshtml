                    //            //$("#txtLOLOorUNLOADING-@a.DocEntry-@i").val(setobj.LOLOorYard);
                    //        } else {
                    //            $("#txtOwner-@a.DocEntry-@i").val("");
                    //            $("#txtYard-@a.DocEntry-@i").val("");
                    //            $("#txtFCLorLCL-@a.DocEntry-@i").val("");
                    //            $("#txtLOLOorUNLOADING-@a.DocEntry-@i").val("");
