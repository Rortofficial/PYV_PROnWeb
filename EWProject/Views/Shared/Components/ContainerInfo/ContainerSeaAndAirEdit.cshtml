            <script defer>
                @Html.Raw("$('#txtOwner2-"+a.DocEntry+"-"+i+"').select2({placeholder: 'Select a Owner',maximumSelectionLength: 1});")
                @Html.Raw("$('#cboContainerExisting-"+a.DocEntry+"-"+i+"').select2({placeholder: 'Select a Container',maximumSelectionLength: 1});")
