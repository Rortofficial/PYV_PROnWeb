            <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["JOB NO."]</div>
            <div class="col-8">
                <input type="text" class="form-control" id="txtJobNo" value="@Model.GetDetailJobSheetTruckings[0].JOBNO" readonly>
            </div>
        </div>
    </div>
</div>
<div class="row mt-2">
    <div class="col-6">
