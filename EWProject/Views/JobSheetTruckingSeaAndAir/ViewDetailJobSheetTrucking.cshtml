            <div class="col-8">
                <input type="number" class="form-control" disabled value="@Model.GetJobSheetTruckings.ObjectHeader.REBATE">
            </div>
        </div>
    </div>
    <div class="col-6">
    </div>
</div>
<div class="row mt-2">
    <div class="col-6">
        <div class="row">
            <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["GRAND TOTAL COSTING"]</div>
            <div class="col-8">
                <input type="number" class="form-control" value="@Model.GetJobSheetTruckings.ObjectHeader.GRANDTOTALCOSTING" readonly>
            </div>
        </div>
    </div>
    <div class="col-6">
        <div class="row">
            <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["GRAND TOTAL SELLING"]</div>
            <div class="col-8">
                <input type="number" class="form-control" value="@Model.GetJobSheetTruckings.ObjectHeader.GRANDTOTALSELLING" readonly>
            </div>
        </div>
    </div>
</div>
<div class="row mt-2">
    <div class="col-6"></div>
    <div class="col-6">
        <div class="row">
            <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["PROFIT"]</div>
            <div class="col-8">
                <input type="number" class="form-control" value="@Model.GetJobSheetTruckings.ObjectHeader.TOTALPROFIT" readonly>
            </div>
        </div>
    </div>
</div>
<div class="row mt-2">
    <div class="col-6">
        <div class="row">
            <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["DUTY TAX AMOUNT"]</div>
            <div class="col-8">
                <input type="number" class="form-control" disabled value="@Model.GetJobSheetTruckings.ObjectHeader.DutyTaxAmountCosting">
            </div>
        </div>
    </div>
    <div class="col-6">
        <div class="row">
            <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["DUTY TAX AMOUNT"]</div>
            <div class="col-8">
                <input type="number" class="form-control" disabled value="@Model.GetJobSheetTruckings.ObjectHeader.DutyTaxAmountCosting">
            </div>
        </div>
    </div>
</div>
<div class="row mt-2">
    <div class="col-6">
        <u style="font-weight:bold">REMARK FOR COSTING</u>
    </div>
    <div class="col-6">
        <u style="font-weight:bold">REMARK FOR SELLING</u>
    </div>
</div>
<div class="row mt-2">
    <div class="col-6">
        <textarea id="remarkForCosting" rows="4" class="form-control" readonly>@Model.GetJobSheetTruckings.ObjectHeader.REMARKSCOSTING</textarea>
    </div>
    <div class="col-6">
        <textarea id="remarkForSelling" rows="4" class="form-control" readonly>@Model.GetJobSheetTruckings.ObjectHeader.REMAKRSSELLING</textarea>
    </div>
</div>
<div class="row mt-2">
    <div class="col-6" id="txtAttachmentFile">
        @foreach (var a in Model.GetJobSheetTruckings.Attachment)
        {
            <div class="row mt-2"><div class="col-8"><input type="text" disabled class="form-control" value="@a.ATTACHMENTNAME"></div><div class="col-4"><a href="@path/mediaUpload/@a.ATTACHMENTNAME" class="btn btn-success">Download</a></div></div>
        }
    </div>
    <div class="col-6">
    </div>
</div>
@section scripts{
    <script>
        $('#TbListItemRevenue').DataTable({
            "ordering": false
        });
        $('#TbListItemCosting').DataTable({
            "ordering": false
        });
    </script>
}


