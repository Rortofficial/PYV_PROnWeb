                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-4">
    </div>
    <div class="col-4"></div>
</div>
<div class="row mt-2">
    <div class="col-1 pr-0">
        <hr />
    </div>
    <div class="col-3 p-0" style="text-align:center">
        <h5>Container Information</h5>
    </div>
    <div class="col-8 pl-0">
        <hr />
    </div>
</div>
@{
    int i = 1;
}
@foreach (var a in Model.Data.ListOfContainerInformations)
{
    <!--Detail Container-->
    <div class="row">
        <div class="col-1">
            <hr />
        </div>
        <div class="col-2">
            <div class="align-content-center">
                @a.ContainerType - @i
            </div>
        </div>
        <div class="col-9">
            <hr />
        </div>
    </div>
    @if (a.ContainerOptionType == "unused")
    {
        <div class="row">
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["TYPE"]</div>
                    <div class="col-8">
                        <select class="form-select" disabled>
                            <option selected value="-1">@a.ContainerOptionType</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-4">
            </div>
            <div class="col-4">
            </div>
        </div>
    }
    else
    {
        <div class="row">
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["TYPE"]</div>
                    <div class="col-8">
                        <select class="form-select" disabled>
                            <option selected value="-1">@a.Type</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-4">
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["Invoice List"]</div>
                    <div class="col-8">
                        <input type="text" value="@a.ListInvoice" disabled />
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-3">
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["CONTAINER TYPE"]</div>
                    <div class="col-8">
                        <input type="text" id="" class="form-control" disabled value="@a.ContainerType" disabled>
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["CONTAINER NO"]</div>
                    <div class="col-8">
                        <input type="text" id="" class="form-control" disabled value="@a.ContainerNo" disabled>
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["OWNER"]</div>
                    <div class="col-8">
                        <input type="text" class="form-control" value="@a.Owner" disabled>
                    </div>
                </div>
            </div>
        </div>
        @if (@a.ContainerType == "EXCHANGE")
        {
            <!--Exchange Container Type-->
            <div class="row mt-3" id="" style="">
                <div class="col-4">
                    <div class="row">
                        <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["CONTAINER TYPE"]</div>
                        <div class="col-8">
                            <input type="text" class="form-control" disabled value="">
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="row">
                        <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["CONTAINER NO"]</div>
                        <div class="col-8">
                            <div class="input-group">
                                <input type="text" id="" class="form-control">
                                <button type="button" class="btn" style="background-color:#01295C;color:white" data-bs-toggle="modal" data-bs-target="#ModalItemPurchaseRequest">
                                    <i class="fa fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="row">
                        <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["OWNER"]</div>
                        <div class="col-8">
                            <input type="text" id="" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <!--End Exchange Container Type-->
        }
        <div class="row mt-2">
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["GW"]</div>
                    <div class="col-8">
                        <input type="text" id="" class="form-control" value="@a.GrossWeight" disabled>
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["YARD"]</div>
                    <div class="col-8">
                        <input type="text" class="form-control" disabled value="@a.Yard">
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-6" style="margin:auto;text-align:left;font-weight:bold;">
                        <input type="text" class="form-control" disabled value="@a.FCL">
                    </div>
                    <div class="col-6">
                        <input type="text" class="form-control" disabled value="@a.LOLO">
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-8">
                <div class="row">
                    <div class="col-2" style="margin:auto;text-align:left;font-weight:bold;">@localizer["TRUCK NO"]</div>
                    <div class="col-10">
                        <div class="row">
                            <div class="col-6">
                                <input type="text" class="form-control" disabled value="@a.TruckProvince">
                            </div>
                            <div class="col-6">
                                <input type="text" class="form-control" disabled value="@a.TruckPlateNo">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["TRUCK TYPE"]</div>
                    <div class="col-8">
                        <input type="text" class="form-control" disabled value="@a.TruckType">
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["BRAND"]</div>
                    <div class="col-8">
                        <input type="text" class="form-control" value="@a.Brand" disabled>
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["COLOR"]</div>
                    <div class="col-8">
                        <input type="text" class="form-control" disabled value="@a.Colors">
                    </div>
                </div>
            </div>
            <div class="col-4">
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-8">
                <div class="row">
                    <div class="col-2" style="margin:auto;text-align:left;font-weight:bold;">@localizer["TRAILER NO"]</div>
                    <div class="col-10">
                        <div class="row">
                            <div class="col-6">
                                <input type="text" class="form-control" value="@a.TrailerProvince" id="" disabled>
                            </div>
                            <div class="col-6">
                                <input type="text" class="form-control" value="@a.TrailerPlateNo" disabled>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <div class="col-4" style="text-align:left;font-weight:bold;">@localizer["TRAILER TYPE"]</div>
                    <div class="col-8">
                        <textarea id="" class="form-control" disabled>@a.TrailerType</textarea>
                    </div>
                </div>
            </div>
        </div>
        <!--Drive 1 Name-->
        <div class="row mt-2">
            <div class="col-6">
                <div class="row">
                    <div class="col-4" style="margin:auto;text-align:left;font-weight:bold;">@localizer["DRIVER NAME1"]</div>
                    <div class="col-8">
                        <input type="text" class="form-control" value="@a.DriverName1" disabled>
