                <h5>List Of AdvancePayment</h5>
            </div>
            <div class="col-8 pl-0">
                <hr />
            </div>
        </div>
        int j = 1;
        @foreach (var purchase in a.listOfAdvancePayments)
        {
            <div class="row mt-2">
                <div class="col-8">
                    <div class="row">
                        <div class="col-6">
                            @purchase.RefNo
                        </div>
                        <div class="col-6">
                            @purchase.Remarks
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <button class="btn" id="btnEdit" onclick="$('#listModalPurchaseRequestAD-@i-@j').css('display','none')"
                style="float:right;background-color:#01295C;color:white;margin-left:5px;border-color:#01295C;">
                        Hide
                    </button>
                    <button class="btn btn-success" style="float:right" id="btnView" onclick="$('#listModalPurchaseRequestAD-@i-@j').css('display','')">View</button>
